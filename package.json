    },
    "homepage": "https://limenius.github.io/liform-react/",
    "dependencies": {
        "ajv": "^5.2.2",
        "bootstrap-sass": "^3.3.7",
        "deepmerge": "^1.5.1",
        "highlight-loader": "^0.7.2",
        "highlight.js": "^9.12.0",
        "html-loader": "^0.5.1",
        "http-client": "^4.3.1",
        "liform-react": "^0.6.8",
        "lodash.forin": "^4.0.1",
        "markdown-loader": "^2.0.1",
        "marked": "^0.3.6",
        "mkdirp": "^0.5.1",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.6",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "router": "^1.3.1"
