        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "eslint": "^4.6.1",
        "eslint-config-rackt": "^1.1.1",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-react": "^7.3.0",
